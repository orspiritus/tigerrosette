{
  "name": "tigerrozetka-backend",
  "version": "1.0.0",
  "description": "Multiplayer backend for TigerRozetka Telegram Mini App",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "migrate": "prisma migrate dev",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:reset": "prisma migrate reset && npm run db:seed"
  },
  "keywords": [
    "telegram",
    "mini-app",
    "game",
    "multiplayer",
    "websocket"
  ],
  "author": "TigerRozetka Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/node": "^24.3.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.0",
    "redis": "^4.6.12",
    "socket.io": "^4.7.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
